package fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	public static void main(String[] args) {
		float a = 1;
		System.out.println(a);
		
		float b = (float) 1.1234567899999;//float nao suporta a partir do "9"
		System.out.println(b);
		
		int c = 124;
		byte d = (byte) c;
		System.out.println(d);
	}
}
